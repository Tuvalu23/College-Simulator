var RequestResponse = (function () {
    function RequestResponse(opts) {
        this.reqResMap = {};
        this.timeout = opts.timeout || 1000 * 3;
        this.transport = opts.transport;
    }
    RequestResponse.prototype.send = function (obj, opts, callback) {
