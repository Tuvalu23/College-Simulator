function getIntroTexts(settings, liveInvalid) {
    var texts = {
        header: settings.introHeader,
        text: settings.introText,
    };
    if (!liveInvalid && settings.introLiveOn && settings.botchatoperators) {
        var introHeader = null;
        var introHeaderLive = settings.introHeaderLive;
        if (introHeaderLive && ~introHeaderLive.indexOf('{name}')) {
            var operators = settings.botchatoperators;
            var operator = settings.botchatoperators[0];
            if (operator) {
                var name = operator.name;
                if (name) {
                    introHeader = introHeaderLive.replace(/\{name\}/g, name);
                }
            }
        }
        else {
            introHeader = introHeaderLive;
        }
        if (introHeader) {
            texts.header = introHeader;
        }
        texts.text = settings.introTextLive;
    }
    return texts;
}
exports.getIntroTexts = getIntroTexts;
function getCustomCss(customCss, settings) {
    if (!customCss)
        customCss = '';
    var defaultCss = '';
    if (settings) {
        var prefix = 'div.ivy-main .ivy-bchat ';
        var prefixPopup = 'div.ivy-main .ivy-popup ';
        if (settings.buttonAiBackgroundColor) {
            defaultCss +=
                prefix +
                    ".ivy-chat-header, ".concat(prefix, " .ivy-bot-btn { color: ") +
                    settings.buttonAiFontColor +
                    ';\nbackground-color: ' +
                    settings.buttonAiBackgroundColor +
                    '}\n ' +
                    '.ivy-chat-close svg, .ivy-chat-menu svg, .ivy-minify-chat svg { fill:' +
                    settings.buttonAiFontColor +
                    ';} ';
            defaultCss += prefix + '.ivy-img:before { border: 1px solid' + settings.buttonAiBackgroundColor + '}\n';
            defaultCss += prefix + '.ivy-img figure { border-color: ' + settings.buttonAiBackgroundColor + '}\n';
            defaultCss += prefixPopup + '.ivy-header a { color: ' + settings.buttonAiBackgroundColor + '}\n';
        }
        if (settings.focusColor)
            defaultCss +=
                'div.ivy-main.ivy-t-circle .ivy-circle.ivy-focused:after { border: 2px solid ' +
                    settings.focusColor +
                    '}\n';
    }
    if (customCss.length == 0 && defaultCss.length == 0)
        return;
    return defaultCss + '\n\n' + customCss;
}
exports.getCustomCss = getCustomCss;


